<div class="popular-frame">

  <div class="thumbnails">
    {{ #artists }}

      <a
        href = "#"
        class = "artist"
        route = "artist"
        args="[{{ id }}]"
      >
        <img src="{{ url }}" ></img>
        <span>{{ name }}</span>
      </a>
    {{ /artists }}
  </div>

  <a route="blog" class="w100" >Go to Blog</a>
</div>
